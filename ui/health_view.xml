<?xml version="1.0"?>
<Window xmlns="https://jonaskloster.dk/xml/tkmvvm"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="https://raw.githubusercontent.com/Joklost/tkmvvm/master/tkmvvm/schema/tkmvvm.xsd"
        height="600" width="800" title="NUTC">
    <Frame grid-column="0" grid-row="0" grid-sticky="nesw" height="50" width="800">
        <Frame grid-row="0" grid-column="0" width="250"/>
        <Entry grid-row="0" grid-column="1" text-binding="{{inputbox}}"/>
        <Button grid-row="0" grid-column="2" command="{submit}" text="Submit"/>
        <!-- <Button grid-row="0" grid-column="3" command="{search}" text="Search"/> -->
    </Frame>
    <Frame grid-column="0" grid-row="1" grid-sticky="nesw" height="500" width="800">
        <!-- <Label grid-sticky="nesw" grid-row="0" grid-column="0" text-binding="{{outputbox}}"/> -->
        <!-- <Entry grid-sticky="nesw"  grid-row="0" grid-column="0" text-binding="{{outputbox}}"/> -->
        <Listbox grid-sticky="nesw"  grid-row="0" grid-column="0" height="500" width="800" list-binding="{{outputbox}}"/>
    </Frame>    
    <!-- <Frame grid-column="0" grid-row="3" grid-sticky="nesw" height="50" width="800">
        <Frame grid-row="0" grid-column="0" width="350"/>
    </Frame> -->
</Window>